<?xml version="1.0"?>
<block>
  <name>matlab_file_payload_py</name>
  <key>beamforming_matlab_file_payload_py</key>
  <category>[beamforming]</category>
  <import>import beamforming</import>
  <make>beamforming.matlab_file_payload_py($file_path)</make>
  <param>
        <name>Payload File</name>
        <key>file_path</key>
        <value>"./data"</value>
        <type>string</type>
  </param>
    <sink>
        <name>in</name>
        <type>complex</type>
    </sink>
  <sink>
        <name>generate</name>
        <type>message</type>
        <optional>1</optional>
  </sink>
  <source>
        <name>payload</name>
        <type>complex</type>
        <vlen>1</vlen>
  </source>
  <source>
        <name>training_signal</name>
        <type>message</type>
        <optional>1</optional>
  </source>
</block>
