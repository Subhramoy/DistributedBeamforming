<?xml version="1.0"?>
<block>
  <name>dynamic_padder_py</name>
  <key>beamforming_dynamic_padder_py</key>
  <category>[beamforming]</category>
  <import>import beamforming</import>
  <make>beamforming.dynamic_padder_py($imag, $real)</make>
  <param>
    <name>IQ Sample</name>
    <key>imag</key>
    <type>float</type>
  </param>
  <param>
    <name>IQ Sample</name>
    <key>real</key>
    <type>float</type>
  </param>
  <sink>
    <name>trigger</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
