<?xml version="1.0"?>
<block>
  <name>CSI_feedback_adapter_py</name>
  <key>beamforming_CSI_feedback_adapter_py</key>
  <category>[beamforming]</category>
  <import>import beamforming</import>
  <make>beamforming.CSI_feedback_adapter_py($file_path, $number_of_tx_antennas, $multicast_IP, $multicast_port, $Tx_ID)</make>
  <param>
    <name>CSI File</name>
    <key>file_path</key>
    <value>dummy_path</value>
    <type>string</type>
  </param>
  <param>
    <name>Number of Tx Antennas</name>
    <key>number_of_tx_antennas</key>
    <value>1</value>
    <type>int</type>
  </param>
  <param>
    <name>Multicast IP</name>
    <key>multicast_IP</key>
    <value>224.3.29.71</value>
    <type>string</type>
  </param>
  <param>
    <name>UDP port</name>
    <key>multicast_port</key>
    <value>10000</value>
    <type>int</type>
  </param>
  <param>
    <name>Transmitter ID</name>
    <key>Tx_ID</key>
    <value>1</value>
    <type>int</type>
  </param>
  <sink>
    <name>trigger</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
  <source>
    <name>beamweight</name>
    <type>message</type>
  </source>
</block>
